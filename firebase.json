{
  "hosting": {
    "public": "dist",
	"cleanUrls": true,
	"trailingSlash": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
	"redirects": [
      {
        "source": "/",
        "destination": "/profile",
        "type": 301
      },
	  {
        "source": "/p",
        "destination": "/profile",
        "type": 301
      }
    ]
  }
}
